         if P.Info = StartingInfo then
            flag := flag + 1;
         end if;
         if flag > 1 then
            exit Traverse_Loop;
         end if;
         if P.Ltag then
            PrintCustomer(P); --Visit Node
            new_line;
            nodeStack.push(P);
            P := P.Llink; --Left
         else
            if nodeStack.numItems = 0 then
               exit Traverse_Loop;
            end if;
            P := nodeStack.pop;
            P := P.Rlink; --Right
         end if;



 nodeStack.push(P);
      while nodeStack.numItems /= 0 loop
         P := nodeStack.pop;
         PrintCustomer(P);
         new_line;
         if P.Rtag then
            nodeStack.push(P.Rlink);
         end if;
         if P.Ltag then
            nodeStack.push(P.Llink);
         end if;
      end loop;
      New_Line;
